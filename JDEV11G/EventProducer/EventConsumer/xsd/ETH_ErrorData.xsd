<xsd:element name="TRANSACTION_RECORD_ID" type="xsd:string" minOccurs="0"/>
<xsd:element name="DOCUMENT_KEY" type="xsd:string" minOccurs="0"/>
<xsd:element name="ERROR_NUMBER" type="xsd:string"/>

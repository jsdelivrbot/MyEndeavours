  <sequence name="main">
     <receive name="ReceivePOCO"
             partnerLink="ETH_CreatePOCOEBM_EMR501_ReqABCSImpl"
             portType="aboabcs:ETH_CreatePOCOEBM_EMR501_ReqABCSImpl"
             operation="ETH_CreatePOCO" variable="ETH_CreatePOCOReqMsg"
             createInstance="yes"/>
        <assign name="Assign1">
            <copy>
                <from expression="string('EMR501')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:SENDER_ID"/>
            </copy>
            <copy>
                <from expression="string(null)"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:RECEIVER_REPROCESS_COUNT"/>
            </copy>
            <copy>
                <from expression="string(null)"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:RECEIVER_ID"/>
            </copy>
            <copy>
                <from expression="string(null)"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:RECEIVER_SYSTEM_ID"/>
            </copy>
            <copy>
                <from expression="string(null)"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:SOURCE_COMPOSITE_INSTANCE"/>
            </copy>
            <copy>
                <from expression="xp20:current-dateTime()"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:CURRENT_DATETIME"/>
            </copy>
            <copy>
                <from expression="string('Kiddan')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET/ns3:HEADER_PROPERTY/@name"/>
            </copy>
            <copy>
                <from expression="string('badhiya')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET/ns3:HEADER_PROPERTY/@value"/>
            </copy>
            <copy>
                <from expression="string(null)"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:B2BINBOUND_RECORD_ID"/>
            </copy>
            <copy>
                <from expression="string('726372')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:MASTER_RECORD_ID"/>
            </copy>
            <copy>
                <from expression="string('77863')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:TRANSACTION_RECORD_ID"/>
            </copy>
            <copy>
                <from expression="string('POID:RELEASE:REVISION:OU')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:DOCUMENT_KEY"/>
            </copy>
            <copy>
                <from expression="string('PO')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:TRANSACTION_TYPE"/>
            </copy>
            <copy>
                <from expression="string('860')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:TRANSACTION_SUBTYPE"/>
            </copy>
            <copy>
                <from expression="string('0')"/>
                <to variable="gvarSOAPETHHeader"
                    query="/ns3:ETHHEADER/ns3:REPROCESS_COUNT"/>
            </copy>
        </assign>
        <assign name="GetTitleandAssignGlobalVariables">
            <copy>
                <from expression="'ETH_Create POCO'"/>
                <to variable="Title"/>
            </copy>
            <copy>
                <from expression="string(null)"/>
                <to variable="gvarB2BInboundRecordId"/>
            </copy>
            <copy>
                <from variable="gvarSOAPETHHeader"
                      query="/ns3:ETHHEADER/ns3:SENDER_ID"/>
                <to variable="gvarSenderID"/>
            </copy>
            <copy>
                <from variable="gvarSOAPETHHeader"
                      query="/ns3:ETHHEADER/ns3:MASTER_RECORD_ID"/>
                <to variable="gvarMasterRecordId"/>
            </copy>
            <copy>
                <from expression="xp20:current-dateTime()"/>
                <to variable="gvarCurrentDateTime"/>
            </copy>
            <copy>
                <from variable="gvarSOAPETHHeader"
                      query="/ns3:ETHHEADER/ns3:TRANSACTION_RECORD_ID"/>
                <to variable="gvarTransactionRecordId"/>
            </copy>
            <copy>
                <from variable="gvarSOAPETHHeader"
                      query="/ns3:ETHHEADER/ns3:DOCUMENT_KEY"/>
                <to variable="gvarDocumentNumber"/>
            </copy>
            <copy>
                <from expression="ora:setCompositeInstanceTitle($gvarDocumentNumber)"/>
                <to variable="gvarCompositeTitle"/>
            </copy>
            <copy>
                <from variable="gvarSOAPETHHeader"
                      query="/ns3:ETHHEADER/ns3:TRANSACTION_TYPE"/>
                <to variable="gvarTransactionType"/>
            </copy>
            <copy>
                <from variable="gvarSOAPETHHeader"
                      query="/ns3:ETHHEADER/ns3:TRANSACTION_SUBTYPE"/>
                <to variable="gvarTransactionSubType"/>
            </copy>
            <copy>
                <from expression="ora:getCompositeName()"/>
                <to variable="gvarCompositeName"/>
            </copy>
            <copy>
                <from expression="ora:getComponentName()"/>
                <to variable="gvarComponentName"/>
            </copy>
            <copy>
                <from expression="ora:getPreference('JDBC_URL')"/>
                <to variable="gvarJDBC_URL"/>
            </copy>
            <copy>
                <from expression="ora:getPreference('JDBC_CONF_JNDI')"/>
                <to variable="gvarJDBC_CONF_JNDI"/>
            </copy>
            <copy>
                <from expression="ora:getComponentInstanceId()"/>
                <to variable="gvarComponentInstanceID"/>
            </copy>
            <copy>
                <from expression="ora:getCompositeInstanceId()"/>
                <to variable="gvarCompositeInstanceId"/>
            </copy>
            <copy>
                <from expression="string('PTTCS')"/>
                <to variable="gvarRegistrationCode"/>
            </copy>
            <copy>
                <from expression="ora:getInstanceId()"/>
                <to variable="InstanceID"/>
            </copy>
            <copy>
                <from expression="ora:getProcessId()"/>
                <to variable="ServiceName"/>
            </copy>
            <copy>
                <from expression="ora:getPreference('messageEncoding')"/>
                <to variable="gvarMessageEncoding"/>
            </copy>
        </assign>
    
     
                <scope name="Scope_TransformationPOCO"
                       variableAccessSerializable="no">
                    <bpelx:annotation>
                        <bpelx:general>
                            <bpelx:property name="userLabel">Scope_TransformationPOCO</bpelx:property>
                        </bpelx:general>
                    </bpelx:annotation>
            <variables>
                <variable name="lvarXSLTParameter"
                          messageType="ebs:ETH_HeaderMsg"/>
                 <variable name="lvarHeaderProperty" element="ns3:HEADER_PROPERTY"/>         
            </variables>
                    <sequence name="Sequence_Transformation">
                <assign name="Assign_XSLTParameter">
          <copy>
            <from variable="gvarSOAPETHHeader" query="/ns3:ETHHEADER"/>
            <to variable="lvarXSLTParameter" part="ETH_HeaderMsg"
                query="/ns3:ETHHEADER"/>
          </copy>
          <copy>
                                <from expression="ora:getCurrentDateTime()"/>
                                <to variable="lvarXSLTParameter"
                                    part="ETH_HeaderMsg"
                                    query="/ns3:ETHHEADER/ns3:CURRENT_DATETIME"/>
                            </copy>
                            <copy>
                                <from expression="string('JDBC_URL')"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@name"/>
                            </copy>
                            <copy>
                                <from variable="gvarJDBC_URL"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@value"/>
                            </copy>
                            <bpelx:append>
                                <bpelx:from variable="lvarHeaderProperty"
                                            query="/ns3:HEADER_PROPERTY"/>
                                <bpelx:to variable="lvarXSLTParameter"
                                          part="ETH_HeaderMsg"
                                          query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET"/>
                            </bpelx:append>
                            <copy>
                                <from expression="string('JDBC_CONF_JNDI')"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@name"/>
                            </copy>
                            <copy>
                                <from variable="gvarJDBC_CONF_JNDI"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@value"/>
                            </copy>
                            <bpelx:append>
                                <bpelx:from variable="lvarHeaderProperty"
                                            query="/ns3:HEADER_PROPERTY"/>
                                <bpelx:to variable="lvarXSLTParameter"
                                          part="ETH_HeaderMsg"
                                          query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET"/>
                            </bpelx:append>
                            <copy>
                                <from expression="string('COMPOSITE_NAME')"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@name"/>
                            </copy>
                            <copy>
                                <from variable="gvarCompositeName"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@value"/>
                            </copy>
                            <bpelx:append>
                                <bpelx:from variable="lvarHeaderProperty"
                                            query="/ns3:HEADER_PROPERTY"/>
                                <bpelx:to variable="lvarXSLTParameter"
                                          part="ETH_HeaderMsg"
                                          query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET"/>
                            </bpelx:append>
                            <copy>
                                <from expression="string('COMPONENT_NAME')"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@name"/>
                            </copy>
                            <copy>
                                <from variable="gvarComponentName"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@value"/>
                            </copy>
                            <bpelx:append>
                                <bpelx:from variable="lvarHeaderProperty"
                                            query="/ns3:HEADER_PROPERTY"/>
                                <bpelx:to variable="lvarXSLTParameter"
                                          part="ETH_HeaderMsg"
                                          query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET"/>
                            </bpelx:append>
                            <copy>
                                <from expression="string('COMPOSITE_INSTANCE_ID')"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@name"/>
                            </copy>
                            <copy>
                                <from variable="gvarCompositeInstanceId"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@value"/>
                            </copy>
                            <bpelx:append>
                                <bpelx:from variable="lvarHeaderProperty"
                                            query="/ns3:HEADER_PROPERTY"/>
                                <bpelx:to variable="lvarXSLTParameter"
                                          part="ETH_HeaderMsg"
                                          query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET"/>
                            </bpelx:append>
                            <copy>
                                <from expression="string('COMPONENT_INSTANCE_ID')"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@name"/>
                            </copy>
                            <copy>
                                <from variable="gvarComponentInstanceID"/>
                                <to variable="lvarHeaderProperty"
                                    query="/ns3:HEADER_PROPERTY/@value"/>
                            </copy>
                            <bpelx:append>
                                <bpelx:from variable="lvarHeaderProperty"
                                            query="/ns3:HEADER_PROPERTY"/>
                                <bpelx:to variable="lvarXSLTParameter"
                                          part="ETH_HeaderMsg"
                                          query="/ns3:ETHHEADER/ns3:HEADER_PROPERTY_SET"/>
                            </bpelx:append>
        </assign>
                <assign name="Xform_POCOABMReqMsg_to_EmersonPurchaseOrderEBMReqMsg">
                            <bpelx:annotation>
                                <bpelx:pattern>transformation</bpelx:pattern>
                            </bpelx:annotation>
                            <copy>
                                <from expression="ora:doXSLTransformForDoc('xsl/FlatFilePurchaseOrder_To_ChangePurchaseOrderEBM.xsl', $ETH_CreatePOCOReqMsg.ETH_CreatePOCO, 'lvarXSLTParameter.ETH_HeaderMsg', $lvarXSLTParameter.ETH_HeaderMsg)"/>
                                <to variable="ETH_CreateEmersonPurchaseOrderEBSReqMsg"
                                    part="ChangePurchaseOrderEBM"/>
                            </copy>
                        </assign>
                    </sequence>
                </scope>
                <scope name="Scope_Invoke_PurchaseOrder"
                       variableAccessSerializable="no">
                    <invoke name="InvokeEmersonPurchaseOrder"
                            inputVariable="ETH_CreateEmersonPurchaseOrderEBSReqMsg"
                            partnerLink="PurchaseOrderEBS"
                            portType="ebs:PurchaseOrderEBS"
                            operation="ChangePurchaseOrder"
                            bpelx:invokeAsDetail="no"/>
                </scope>
   
  </sequence>
</process>
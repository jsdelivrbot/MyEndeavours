       <case condition="(bpws:getVariableData('gvarTransactionType') = 'SINV')">
          <bpelx:annotation>
            <bpelx:general>
              <bpelx:property name="userLabel">Transaction is Invoice</bpelx:property>
            </bpelx:general>
          </bpelx:annotation>
          <sequence name="Sequence_InvoiceProvider">

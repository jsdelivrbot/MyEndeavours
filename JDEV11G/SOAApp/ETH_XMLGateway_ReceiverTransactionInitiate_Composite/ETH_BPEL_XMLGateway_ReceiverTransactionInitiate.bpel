                <to variable="lvarRouteToASNProvider" part="request"
                    query="/ns13:ShowAdvanceShipmentNoticeEBM"/>
              </copy>
            </assign>
            <invoke name="Invoke_RouteTo_ASNProvider"
                    inputVariable="lvarRouteToASNProvider"
                    partnerLink="ETH_Mediator_RouteTo_ASNProvider.ETH_Mediator_RouteTo_ASNProvider"
                    portType="ns11:execute_ptt" operation="execute"
                    bpelx:invokeAsDetail="no"
                    bpelx:inputHeaderVariable="SOAPHeader"/>
          </sequence>
        </case>
        
        <case condition="(bpws:getVariableData('gvarTransactionType') = 'SACK')">
          <bpelx:annotation>
            <bpelx:general>
              <bpelx:property name="userLabel">Transaction is POACK</bpelx:property>

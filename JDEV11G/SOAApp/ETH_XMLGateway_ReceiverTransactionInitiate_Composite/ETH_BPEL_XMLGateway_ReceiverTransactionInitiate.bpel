                <from expression="oraext:parseXML(bpws:getVariableData('gvarRcvMessagePayload'))"/>
                <to variable="lvarRouteToInvoiceProvider" part="request"
                    query="/ns5:ProcessInvoiceEBM"/>
              </copy>
            </assign>
            <invoke name="Invoke_RouteTo_InvoiceProvider"
                    inputVariable="lvarRouteToInvoiceProvider"
                    partnerLink="ETH_Mediator_RouteTo_InvoiceProvider.ETH_Mediator_RouteTo_InvoiceProvider"
                    portType="ns1:execute_ptt" operation="execute"
                    bpelx:inputHeaderVariable="SOAPHeader"
                    bpelx:invokeAsDetail="no"/>
          </sequence>
        </case>
        <case condition="(bpws:getVariableData('gvarTransactionType') = 'SASN')">
          <bpelx:annotation>
            <bpelx:general>
              <bpelx:property name="userLabel">Transaction is ASN</bpelx:property>

                  <from expression="string('TRANSACTION_FAILURE')"/>
                  <to variable="gvarFaultState"/>
                </copy>
                <copy>
                  <from variable="gvarDocumentkey"/>
                  <to variable="outputVariable" part="payload" query="/client:processResponse/client:result"/>
                </copy>
              </assign>
              <empty name="Transaction_Received" id="BpEmp0"/>
            </sequence>
          </case>
          <case condition="bpws:getVariableData('lvargetDBResponse') = 'DB_FAILURE'">
            <bpelx:annotation>
              <bpelx:general>
                <bpelx:property name="userLabel">DB_FAILURE</bpelx:property>
              </bpelx:general>
            </bpelx:annotation>
            <sequence name="Sequence_Handle_DBFailure_Response" id="BpSeq7">
              <assign name="Assign_Data_to_lvarEnqErrorDataInput" id="BpAss9">
                <copy>
                  <from expression="string('DEAD')"/>
                  <to variable="lvarEnqErrorDataInput" part="body" query="/ns3:ETH_ErrorData/ns3:ERROR_SOURCE"/>

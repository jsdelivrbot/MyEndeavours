        <assign name="Assign_lvarGetDBTransactionOutput_to_GlobalVariables" id="BpAss5">
          <copy>
            <from variable="lvarGetDBTransactionOutput" part="OutputParameters" query="/ns2:OutputParameters/ns2:P_MASTER_RECORD_ID"/>

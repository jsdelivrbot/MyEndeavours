          </copy>
          <copy>
            <from variable="lvarGetDBTransactionOutput" part="OutputParameters" query="/ns2:OutputParameters/ns2:P_TP_ID_SENDER"/>
            <to variable="gvarSenderId"/>
          </copy>
          <copy>
            <from variable="lvarGetDBTransactionOutput" part="OutputParameters" query="/ns2:OutputParameters/ns2:P_TRANSACTION_SUBTYPE"/>
            <to variable="gvarTransactionSubType"/>
          </copy>
          <copy>
            <from variable="lvarGetDBTransactionOutput" part="OutputParameters" query="/ns2:OutputParameters/ns2:P_DOCUMENT_KEY"/>

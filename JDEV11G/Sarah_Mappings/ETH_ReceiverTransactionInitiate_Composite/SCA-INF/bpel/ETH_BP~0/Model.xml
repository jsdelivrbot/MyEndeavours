                <to variable="gvarEnqErrorDataInput" part="body" query="/ns3:ETH_ErrorData/ns3:COMPOSITE_NAME"/>
              </copy>
              <copy>
                <from variable="gvarComponentName"/>
                <to variable="gvarEnqErrorDataInput" part="body" query="/ns3:ETH_ErrorData/ns3:COMPONENT_NAME"/>
              </copy>
              <copy>
                <from variable="gvarCompositeInstanceId"/>

            <throw name="RethrowFaultforCallback" faultName="ns15:faultCallback" id="BpThw0"/>
          </sequence>
        </catchAll>
      </faultHandlers>
      <sequence name="Sequence_Get_Most_Eligible_Transaction" id="BpSeq4">
        <assign name="Assign_Data_to_lvarGetDBTransactionInput" id="BpAss4">
          <copy>
            <from variable="gvarComponentName"/>
            <to variable="lvarGetDBTransactionInput" part="InputParameters" query="/ns2:InputParameters/ns2:P_CALLER_NAME"/>
          </copy>
          <copy>
            <from expression="2"/>
            <to variable="lvarGetDBTransactionInput" part="InputParameters" query="/ns2:InputParameters/ns2:P_SUBMISSION_POINT"/>

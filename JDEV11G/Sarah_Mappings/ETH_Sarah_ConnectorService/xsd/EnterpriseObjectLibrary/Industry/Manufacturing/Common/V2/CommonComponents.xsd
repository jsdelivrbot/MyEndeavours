	<!-- =====      Activity-->
	<xsd:complexType name="ActivityType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the activity</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Detail explanation of what the activity is</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of the activity such as CRF (case report form), procedure </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Cost" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The cost associated with performance of the activity</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Custom" type="corecomcust:CustomActivityType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Activity" type="ActivityType">
		<xsd:annotation>
			<xsd:documentation>An Activity represents a definition of what the action is (not including its usage and occurrence).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ActivityEventType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:element name="PlannedStartDateTime" type="DateTimeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date time at which the activity has been planned or scheduled to take place</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpectedStartDateTime" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>When the Planned Start Date Time is no longer realistic, this is the new expected date of when the activity will occur. The Planned Start Date Time does not change for tracking purposes.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ActualStartDateTime" type="DateTimeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This record the date time at which the activity has taken place</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ActualCompletionDateTime" type="DateTimeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date time at which the activity has been completed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AssignedWorkerReference" minOccurs="0"/>
			<xsd:element ref="ActivityEventStatusHistory" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="Activity" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="ActivityEvent" type="ActivityEventType">
		<xsd:annotation>
			<xsd:documentation>An ActivityEvent captures occurrence information of an Activity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ActivityEventStatusHistoryType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:element ref="StatusHistory" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="ActivityEventStatusHistory" type="ActivityEventStatusHistoryType">
		<xsd:annotation>
			<xsd:documentation>This component allows for keeping track of history of statuses of an activity event/occurrence</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Allowance-->
	<xsd:element name="Allowance" type="AllowanceType">
		<xsd:annotation>
			<xsd:documentation>A component of pricing that decreases the value of a price</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AllowanceType">
		<xsd:sequence>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identify the type of allowance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="BasisAmount" type="AmountType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The amount on which the allowance is calculated. This may be different from the line amount</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="BasisQuantity" type="QuantityType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The quantity used for calculating the allowance This may be different from the line quantity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="Amount" type="AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Allowance Amount in the transactional currency</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="CalculationPercent" type="PercentType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The percentage applied to the Basis Amount or Basis Quantity to calculate the allowance. Either the CalculationPercent or the CalculationNumericRate can be specified</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="Calculation" type="TextType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Formula used to calculate the allowance described as text</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="ReasonCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reason for applying the allowance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurrencyExchange" minOccurs="0"/>
			<xsd:element ref="EffectiveTimePeriod" minOccurs="0"/>
			<xsd:element name="PrePaidIndicator" type="IndicatorType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An indication of whether or not the allowance is prepaid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="TextType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- =====      Appointment Window-->
	<xsd:element name="AppointmentWindow" type="AppointmentWindowType">
		<xsd:annotation>
			<xsd:documentation>Appointment time slot or window in which the work, action, or event etc. will begin including.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AppointmentWindowType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:element ref="StartTimePeriod" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The time window in which the work, event, action, etc. is expected to begin.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperationalPreferenceCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The priority or preference level assigned  this appointment time period.  This is a preference level from the companies perspective.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Custom" type="corecomcust:CustomAppointmentWindowType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- =====      Attachment-->
	<xsd:element name="Attachment" type="AttachmentType">
		<xsd:annotation>
			<xsd:documentation>An attachment provides the  means to include additional infornation about a business object or component that is not part of the  the definition of the object or component. An attachment may be an embedded binary data object or a reference to a document or file located at a location that is accessible to all parties that are processing the attachment. Examples are the paper specifications and drawings attached to a business object or component to clearly communicate design requirements. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AttachmentType">
		<xsd:sequence>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of attachment, e.g., URL, TEXT, Grapthics, Repository File, Folder</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PurposeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Primary purpose of the attachment, e.g., Shipping, Manufacturing, Invoicing.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the attachment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Title" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Title of the attachment. This is not the file name e.g. the title of a document is different from the file name of the document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SizeQuantity" type="QuantityType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Size of the attachment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreationDateTime" type="DateTimeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date Time of creation of the document in the system of record</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="EmbeddedData" type="BinaryObjectType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Element holding data embedded directly in the BOD instance</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="URI" type="URIType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>URI indicating where the attachment is located</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="Description" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Description of the attachment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Note" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="CheckFrontImageAttachment" type="AttachmentType">
		<xsd:annotation>
			<xsd:documentation>Image that represents the front side of a Check</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CheckBackImageAttachment" type="AttachmentType">
		<xsd:annotation>
			<xsd:documentation>Image that represents the back side of a Check</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Accounting Distribution-->
	<xsd:complexType name="AccountingDistributionType">
		<xsd:sequence>
			<xsd:element name="LedgerID" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the financial ledger to be used when updating balances</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of the Distribution. Sample values are : Planned, Prepayment and Standard</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccountClassCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Describe the Account Class for the distribution. Examples are Revenue, Receivable, Freight, Tax, Unearned Revenue, Unbilled Receivable, Finance Charges etc</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AllocationTypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates how the distribution has been allocated. Examples are Amount, Percent</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccountingDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the date on which the transactions will be accounted in GL</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Percentage" type="PercentType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the percentage of the Line Price applicable to the distribution line. This is used to derive the Amount on the distribution line</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Quantity" type="QuantityType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the quantity of the associated service or item that is used to calculate the distribution amount</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The amount on this distribution line</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EncumberedQuantity" type="QuantityType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Quantity of the distribution that is encumbered</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EncumberedAmount" type="AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Encumbered amount for distribution</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UnencumberedAmount" type="AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Amount unencumbered on the distribution</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EncumberedIndicator" type="IndicatorType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the distribution has been encumbered</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AllowEncumberanceIndicator" type="IndicatorType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the distribution should be encumbered</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GLEntityID" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A G/L entity is the smallest organizational unit for which individual financial statements must be drawn up according to relevant commercial law.



It is normally the primary balancing segment of the GL Account structure.  Typically, this is the owning entity for the transaction. 



Synonyms are Business Unit, Fund, G/L Company, Joint Venture, Non-Profit Association Name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="GLAccount" minOccurs="0"/>
			<xsd:element name="CostCenterCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Cost Center is an accumulator of cost information that may be an organizational unit or area of responsibility.  It is an organization code that allows a grouping of expenses and costs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProfitCenterCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Profit Center is an accumulator of revenue information that may be an organizational unit or area of responsibility.  A profit center can be used for internal management control.  Profit centers can calculate operating profit using the cost-of-sales accounting and/or period accounting methods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ProjectReference" minOccurs="0"/>
			<xsd:element ref="ContractLineReference" minOccurs="0"/>
			<xsd:element ref="WIPEntityReference" minOccurs="0"/>
			<xsd:element ref="WIPEntityLineReference" minOccurs="0"/>
			<xsd:element ref="WIPOperationReference" minOccurs="0"/>
			<xsd:element ref="WIPOperationResourceReference" minOccurs="0"/>
			<xsd:element ref="BusinessUnitReference" minOccurs="0"/>
			<xsd:element ref="RequisitionDistributionReference" minOccurs="0"/>
			<xsd:element ref="CurrencyExchange" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="AccountingDistribution" type="AccountingDistributionType">
		<xsd:annotation>
			<xsd:documentation>An Accounting Distribution identifies how monetary amounts are distributed accross different financial entities within an organization</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Address-->
	<xsd:complexType name="AddressType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:element name="FormatCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A code specifying the format of this address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineOne" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The first free form line, expressed as text, of an address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineTwo" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The first second form line, expressed as text, of an address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineThree" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The third free form line, expressed as text, of an address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineFour" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The fourth free form line, expressed as text, of an address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineFive" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The fifth free form line, expressed as text, of an address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineSix" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The sixth free form line, expressed as text, of an address (use a lot for 
localization for certain countries)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineSeven" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The seventh free form line, expressed as text, of an address (use a lot for 
localization for certain countries)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineEight" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The eighth free form line, expressed as text, of an address (use a lot for 
localization for certain countries)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineNine" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The ninth free form line, expressed as text, of an address (use a lot for 
localization for certain countries)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Building" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A building name or number. An office park or campus may have several buildings that are distinguished by the building name or number.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Floor" type="StringType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A floor in a building
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Area" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Refers to an area of the building such as left wing, right wing etc.  
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CityName" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The name, expressed as text, of the city, town or village of this address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StateName" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the state in which  the city falls, where applicable</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProvinceName" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the province where the address is located, if applicable</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RuralRoute" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Rural Route Number for the address, if applicable</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CountyName" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>County Name, if applicable</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CountryCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The unique identifier of a country for this address (Reference ISO 3166 and UN/ECE Rec 3)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CountrySubDivisionID" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An identification of a country sub-division of this address such as a department in France. To be used only if there is no named term that represents the countrysubdivision concept being expressed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CitySubDivisionName" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A name, expressed as text, of a sub-division of a city for this address, for example a district or borough.  To be used only if there is no named term that represents the citysubdivision concept being expressed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeliveryPointCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An in-house mail location, expressed as code, for this address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeliveryPointBarCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A three digit field used in CASS (Coding Accuracy Support System) along with ZIP, ZIP+4 and CRRT Code to automate the postal sortation process and maximize postage discounts.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeliveryPointTypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the type of delivery point. ie. 
Residential Curb, Residential NDCBU (Neighborhood Delivery and Collection Box 
Unit), Residential Central, Residential Other, Business Curb, Business NDCBU, 
Business Central, Business Other, Exclude from Delivery Statistics...etc </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProcessingCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Postal Adress Processing Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MoveTypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>code that identifies what type of move. For examlpe, NCOALINK-USPS Product designed to return the most current address if the "mover" filed an NCOA Move card with the USPS and the previous address is an exact match to 
the USPS NCOA LINK Database a new address will be returned.  For examlpe: F = Family, I = Individual, B = Business or Firm,  Blank = No NCOALink match. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MoveEffectiveDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date the Move is in effect </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AttentionOf" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The name, expressed as text, of a person or department in the organization to whom incoming mail is marked with words such as 'for the attention of' or 'FAO' or 'ATTN' for this address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CareOf" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The name, expressed as text, of a person or organization at this address to whom incoming mail is marked with words such as 'care of' or 'C/O'</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PostalCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A code specifying the postalcode of the address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LongPostalCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A full postalcode of the address, e.g., 94070-3541</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GEOCodeID" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A unique code that represents a geospatial coordinate measurement of an exact geographic location and time at, below, or above the surface of the earth</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InCityLimitIndicator" type="IndicatorType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether or not this address is within the taxing boundary of the jurisdiction that's provided in the address.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimeZoneCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code indicating the time zone of the address, e.g., EST, PST, UTC-8, UTC-5</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Custom" type="corecomcust:CustomAddressType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Address" type="AddressType">
		<xsd:annotation>
			<xsd:documentation>The location at which a particular organization or person may be found or reached</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CardHolderAddress" type="AddressType">
		<xsd:annotation>
			<xsd:documentation>Address of the holder of a credit or debit card</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CardHolderBillingAddress" type="AddressType">
		<xsd:annotation>
			<xsd:documentation>Billing Address of a Credit oe Debit Card Holder</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PayeeAddress" type="AddressType">
		<xsd:annotation>
			<xsd:documentation>The address associated with a Payee in a payment transaction</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--=====   BankAccount-->
	<xsd:complexType name="BankAccountType">
		<xsd:sequence>
			<xsd:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name on the Bank Account</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccountNumber" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bank Account Number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IBANNumber" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>International Bank Account Number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrencyCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>CurrencyCode in which the account is maintained</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CheckDigits" type="StringType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Check Digits used to verify the IBAN Number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Account Type. Examples are Checking, Savings etc</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankAccountHolderPartyReference" minOccurs="0"/>
			<xsd:element ref="BankPartyReference" minOccurs="0"/>
			<xsd:element ref="BankBranchReference" minOccurs="0"/>
			<xsd:element name="Description" type="TextType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="BankAccount" type="BankAccountType">
		<xsd:annotation>
			<xsd:documentation>A bank account is a monetary account with a banking institution recording the balance of money for a customer</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ChargeProcessingBankAccount" type="BankAccountType">
		<xsd:annotation>
			<xsd:documentation>Reference to the Bank Account that will be used to process the charges incurred in a financial transaction</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PayeeBankAccount" type="BankAccountType">
		<xsd:annotation>
			<xsd:documentation>Reference to the Bank Account of a Payee in a financial transaction.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PayFromBankAccount" type="BankAccountType">
		<xsd:annotation>
			<xsd:documentation>Reference to a Bank Account from where the money is withdrawn for a payment in a financial transaction</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DefaultCustomerBankAccount" type="BankAccountType">
		<xsd:annotation>
			<xsd:documentation>This is the customer bank account to be used as the default one for payment transactions</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Bank Draft-->
	<xsd:complexType name="BankDraftType">
		<xsd:sequence>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of Bank Draft</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccountHolderName" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the account holder</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankName" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the Bank</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Number" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unique Number identifying the Bank Draft</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RoutingNumber" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bank Routing Number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Memo" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Memo on the Draft</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssuedDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Issued Date for the payment </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Custom" type="corecomcust:CustomBankDraftType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="BankDraft" type="BankDraftType">
		<xsd:annotation>
			<xsd:documentation>A check drawn by one bank against funds deposited into its account at another bank, authorizing the second bank to make payment to the individual or organization named in the draft</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Bank Payment-->
	<xsd:complexType name="BankPaymentType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:element name="ActionTypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of EFT (Electronic Funds Transfer) transaction attempted.  Sample Values : DEBIT, CREDIT, VERIFY, VALIDATE</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InstructionCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unique identification of the Bank payment Instruction that the PayFrom party instructs the bank</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SettlementPriorityCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The priority with which the financial institution or payment system should settle payment for this document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SequenceTypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the direct debit sequence i.e. First, Recurrent, Final or One-off etc</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Payment Reason code determining the purpose of the payment. Usage: This code helps to process and fulfill the payment properly</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationMethodCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Method by which the payer authorized the transaction.  Example of some values can be: WRITTEN, INTERNET_FORM</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeliveryMethodCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Method by which the transaction is delivered.  Example of some values can be: ACH, FASCIMILE</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransferTypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bank account payment transfer type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReferenceCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Payment system-provided reference code for this payment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ChargeBearerCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bearer of bank account charges.  Specifies which party/parties will bear the charges associated with the processing of the payment transaction. Example of some values can be: Credit, Debit, Shared, Service Level</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ChargeAmount" type="AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bank charge amount in payer's Bank account currency</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PayFromBankAccount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Payer's Bank Account</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PayeeBankAccount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Payee's Bank Account</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ChargeProcessingBankAccount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bank Account that pays the Bank Charge amount, if different from the Payer's Bank Account              </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PaymentDebitCommunication" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Contact information related to the Debit Notification        </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PaymentRemittanceCommunication" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Contact information related to the Remittance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="ServiceLevelCode" type="CodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Agreement of Service between the payFrom party and the payee party under which the transfer should be processed. For SEPA the service level code must be SEPA. The other two options are EBA Priority Service and Same Day Value. Alternatively the company and the bank can use mutually agreed service level</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="ClearingChannelCode" type="CodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Clearing channel to be used for the transaction. Valid values are : Book Transfer, Mass Payment Net System, Real Time Gross Settlement System, Real Time Net Settlement System</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element ref="PaymentDirectDebitMandate" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Mandate for the Direct Debit in case of a Direct debit payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="Check" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Details of the check in case of a check payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="Custom" type="corecomcust:CustomBankPaymentType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="BankPayment" type="BankPaymentType">
		<xsd:annotation>
			<xsd:documentation>Bank Account transaction details for the cases where payment method is : Direct Debit, Check or EFT    </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Beneficiary-->
	<xsd:complexType name="BeneficiaryType">
		<xsd:sequence>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> 
Type of Beneficiary, e.g., primary or contingent
                                </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SplitPercent" type="PercentType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> 
Percentage dued to this beneficiary
                                </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element ref="BeneficiaryPersonParty" minOccurs="0"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="BeneficiaryOrganizationParty" minOccurs="0"/>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="Custom" type="corecomcust:CustomBeneficiaryType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Beneficiary" type="BeneficiaryType">
		<xsd:annotation>
			<xsd:documentation> 
This component represents information about a beneficiary, e.g., beneficiary of an insurance</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BeneficiaryOrganizationParty" type="BeneficiaryOrganizationPartyType">
		<xsd:annotation>
			<xsd:documentation>Refers to a beneficaiary which is an organization</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="BeneficiaryOrganizationPartyType">
		<xsd:sequence>
			<xsd:element ref="OrganizationPartyReference" minOccurs="0"/>
			<xsd:element name="Custom" type="corecomcust:CustomBeneficiaryOrganizationPartyType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="BeneficiaryPersonParty" type="BeneficiaryPersonPartyType">
		<xsd:annotation>
			<xsd:documentation>Refers to a beneficaiary which is a individual                          </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="BeneficiaryPersonPartyType">
		<xsd:sequence>
			<xsd:element ref="PersonPartyReference" minOccurs="0"/>
			<xsd:element name="Custom" type="corecomcust:CustomBeneficiaryPersonPartyType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- =====      Payment Debit Communication (subcomponent of Payment)-->
	<xsd:complexType name="PaymentDebitCommunicationType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element ref="PhoneCommunication" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Phone Communication used by the payee to deliver the Debit notification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="FaxCommunication" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Fax Communication used by the payee to deliver the Debit notification       </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="EmailCommunication" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Email Communication used by the payee to deliver the Debit notification</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="Custom" type="corecomcust:CustomPaymentDebitCommunicationType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="actionCode" type="ActionCodeType"/>
	</xsd:complexType>
	<xsd:element name="PaymentDebitCommunication" type="PaymentDebitCommunicationType">
		<xsd:annotation>
			<xsd:documentation>Contact information related to the Debit Notification</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Payment Direct Debit Mandate (subcomponent of Payment)-->
	<xsd:complexType name="PaymentDirectDebitMandateType">
		<xsd:sequence>
			<xsd:element name="AuthorizationMethodCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Mandate authorization method. Example values are : Phone, Web, Written etc</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizedIndicator" type="IndicatorType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag indicating whether the payer has explicitly granted the mandate or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SignatureDateTime" type="DateTimeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date on which the mandate has been signed by the payer party</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ElectronicSignature" type="StringType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Digital signature as provided by the payer party</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FrequencyCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frequency of direct debit. Example values are :Daily, Weekly, Monthly etc.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmendmentIndicator" type="IndicatorType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag indicating whether the underlying mandate has been amended or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="EffectiveTimePeriod" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>First Collection Date and Final Collection Date as authorized by the Mandate</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="MandateSignerPartyReference" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Credit Party that signs the Direct Debit Mandate              </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="OriginalDirectDebitMandateReference" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Original Mandate Reference for the case where amendments are made to the original mandate</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Custom" type="corecomcust:CustomPaymentDirectDebitMandateType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="PaymentDirectDebitMandate" type="PaymentDirectDebitMandateType">
		<xsd:annotation>
			<xsd:documentation> Mandate for the Direct Debit in case of a Direct debit payment      </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Payment Remittance Communication (subcomponent of Payment)-->
	<xsd:complexType name="PaymentRemittanceCommunicationType">
		<xsd:sequence>
			<xsd:element ref="Identification" minOccurs="0"/>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element ref="PhoneCommunication" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Phone Communication used to deliver the Remittance Advice</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="FaxCommunication" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Fax Communication used to deliver the Remittance Advice</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="EmailCommunication" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Email Communication used to deliver the Remittance Advice</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="Custom" type="corecomcust:CustomPaymentRemittanceCommunicationType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="actionCode" type="ActionCodeType"/>
	</xsd:complexType>
	<xsd:element name="PaymentRemittanceCommunication" type="PaymentRemittanceCommunicationType">
		<xsd:annotation>
			<xsd:documentation>Contact information related to the Remittance</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Certification-->
	<xsd:complexType name="CertificationType">
		<xsd:sequence>
			<xsd:element name="Name" type="NameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the certification</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssueDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date on which the certification was issued</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpirationDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Expiration Date for the certification if applicable</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssuingAuthorityID" type="IdentifierType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Authority that issued the certification</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QualifyingGrade" type="StringType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Grade applicable for the certification - e.g. First Class,  Distinction etc</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Certification" type="CertificationType"/>
	<!-- =====      Charge-->
	<xsd:complexType name="ChargeType">
		<xsd:sequence>
			<xsd:element name="TypeCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of Charge</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ChargeFrequencyCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Frequency of Charge. Example monthly,yearly,quarterly</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="BasisAmount" type="AmountType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The amount on which the charge is calculated. This may be different from the line amount</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="BasisQuantity" type="QuantityType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The quantity used for calculating the charge. This may be different from the line quantity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="Amount" type="AmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Charge Amount in the transactional currency</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="CalculationPercent" type="PercentType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The percentage applied to the Basis Amount or Basis Quantity to calculate the charge. Either the CalculationPercent or the CalculationNumericRate can be specified</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="Calculation" type="TextType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Formula used to calculate the charge described as text</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="DiscountAmount" type="AmountType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The Amount by which the price is discounted</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="DiscountPercent" type="PercentType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The percentage by which the price is discounted
</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="ReasonCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reason for applying the charge, if any</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurrencyExchange" minOccurs="0"/>
			<xsd:element ref="TimePeriod" minOccurs="0"/>
			<xsd:element name="PrePaidIndicator" type="IndicatorType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>An indication of whether or not the allowance is prepaid</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Description of the charge</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="UnitListPrice" minOccurs="0"/>
			<xsd:element ref="UnitSalePrice" minOccurs="0"/>
			<xsd:element name="DiscountMethodCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of Discount Method. Example Amount or Percent</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Charge" type="ChargeType">
		<xsd:annotation>
			<xsd:documentation>A component of pricing that increases the value of a price</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FreightCharge" type="ChargeType">
		<xsd:annotation>
			<xsd:documentation>Infrormation about Freight Charge </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- =====      Communication-->
	<xsd:complexType name="CommunicationType" abstract="true">
		<xsd:sequence>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="CompleteNumber" type="TextType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>The complete number without separation between country code, area code etc</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="CountryCode" type="CodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The country dialing code for a communication number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AreaCode" type="CodeType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The area dialing code for a communication number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LocalNumber" type="TextType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>The commication number, not including country dialing or area dialing codes</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExtensionNumber" type="TextType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>The extension of the assoicated communication number</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Access" type="TextType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>The text that permits access to the electronic network of the associated communication number such as telephone network, for example 9, *70.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element name="URI" type="URIType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>URI in the case of email communication</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="HTMLPreferredIndicator" type="IndicatorType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates that the communication is preferred in HTML format.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="Address" minOccurs="0"/>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="UseCode" type="CodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code identifying the usage of the contact point e.g. business, private, etc</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Preference" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Communication" type="CommunicationType">
		<xsd:annotation>
			<xsd:documentation>An abstract  representation of the different means of getting in touch or communicating verbally or in writing with a party</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

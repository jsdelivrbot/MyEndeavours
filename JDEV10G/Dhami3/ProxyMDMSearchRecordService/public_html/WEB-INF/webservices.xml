        <webservice-description-name>ProxyMDMSearchRecord</webservice-description-name>
        <wsdl-file>WEB-INF/wsdl/ProxyMDMSearchRecord.wsdl</wsdl-file>
        <jaxrpc-mapping-file>WEB-INF/ProxyMDMSearchRecord-java-wsdl-mapping.xml</jaxrpc-mapping-file>
        <port-component>

            <wsdl-port xmlns:wsa0="http://www.gssamerica.com/mdm/services/ProxyMDMSearchRecord/">wsa0:ProxyMDMSearchRecordSOAP</wsdl-port>
            <service-endpoint-interface>com.gssamerica.mdm.services.searchproxy.MDMSearchRecord</service-endpoint-interface>
            <service-impl-bean>

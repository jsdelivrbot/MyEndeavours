<oracle-webservices xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://xmlns.oracle.com/oracleas/schema/oracle-webservices-10_0.xsd">
    <webservice-description name="RejectionHandlerService">
        <port-component name="RejectionHandlerPort">
            <operations>
                <operation name="handleError" input="{http://xmlns.oracle.com/pcbpel/errorHandling}RejectedMessage"/>
            </operations>
        </port-component>
    </webservice-description>
</oracle-webservices>

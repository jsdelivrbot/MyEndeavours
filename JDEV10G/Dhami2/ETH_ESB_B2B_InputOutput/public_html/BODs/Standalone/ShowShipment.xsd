			<xsd:group ref="FreeFormTextGroup"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContainerBaseType">
		<xsd:sequence>
			<xsd:group ref="ContainerInstanceIDsGroup"/>

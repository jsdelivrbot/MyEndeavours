		<xsd:attribute name="type" type="NormalizedStringType">
			<xsd:annotation>
				<xsd:documentation>Indicates the type of container.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ConfirmationResponseCodeType">

        <qname-scope>complexType</qname-scope>
        <variable-mapping>
            <java-variable-name>system</java-variable-name>
            <xml-element-name>process</xml-element-name>
        </variable-mapping>
    </java-xml-type-mapping>
    <java-xml-type-mapping>
        <java-type>com.emerson.schemas.common.envelope.SystemType[]</java-type>
        <anonymous-type-qname>{http://schemas.emerson.com/common/envelope}routingInfoType&gt;process[0,unbounded]</anonymous-type-qname>
        <qname-scope>complexType</qname-scope>
    </java-xml-type-mapping>

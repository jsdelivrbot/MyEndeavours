        <qname-scope>complexType</qname-scope>
        <variable-mapping>
            <java-variable-name>faultSeverity</java-variable-name>
            <xml-element-name>faultSeverity</xml-element-name>
        </variable-mapping>
        <variable-mapping>
            <java-variable-name>faultCode</java-variable-name>
            <xml-element-name>faultCode</xml-element-name>
        </variable-mapping>
        <variable-mapping>
            <java-variable-name>faultMessage</java-variable-name>
            <xml-element-name>faultMessage</xml-element-name>
        </variable-mapping>
        <variable-mapping>

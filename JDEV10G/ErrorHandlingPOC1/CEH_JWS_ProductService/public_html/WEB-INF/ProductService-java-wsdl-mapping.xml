            <port-name>ProductPort</port-name>
            <java-port-name>ProductPort</java-port-name>
        </port-mapping>
    </service-interface-mapping>
    <service-endpoint-interface-mapping>
        <service-endpoint-interface>com.emerson.services.product.ProductPortType</service-endpoint-interface>
        <wsdl-port-type xmlns:portTypeNS="http://services.emerson.com/product">portTypeNS:ProductPortType</wsdl-port-type>
        <wsdl-binding xmlns:bindingNS="http://services.emerson.com/product">bindingNS:ProductBinding</wsdl-binding>
        <service-endpoint-method-mapping>
            <java-method-name>processProduct</java-method-name>
            <wsdl-operation>processProduct</wsdl-operation>
            <method-param-parts-mapping>
                <param-position>0</param-position>
                <param-type>com.emerson.schemas.product.ProductType</param-type>
                <wsdl-message-mapping>
                    <wsdl-message xmlns:wsdlMsgNS="http://services.emerson.com/product">wsdlMsgNS:RequestMessage</wsdl-message>
                    <wsdl-message-part-name>requestPart</wsdl-message-part-name>
                    <parameter-mode>IN</parameter-mode>
                </wsdl-message-mapping>
            </method-param-parts-mapping>
            <method-param-parts-mapping>
                <param-position>1</param-position>
                <param-type>com.emerson.schemas.common.envelope.HeaderType</param-type>
                <wsdl-message-mapping>
                    <wsdl-message xmlns:wsdlMsgNS="http://services.emerson.com/product">wsdlMsgNS:HeaderMessage</wsdl-message>
                    <wsdl-message-part-name>headerPart</wsdl-message-part-name>
                    <parameter-mode>IN</parameter-mode>
                    <soap-header />
                </wsdl-message-mapping>
            </method-param-parts-mapping>
            <wsdl-return-value-mapping>
                <method-return-value>java.lang.String</method-return-value>
                <wsdl-message xmlns:wsdlMsgNS="http://services.emerson.com/product">wsdlMsgNS:ResponseMessage</wsdl-message>
                <wsdl-message-part-name>responsePart</wsdl-message-part-name>
            </wsdl-return-value-mapping>
        </service-endpoint-method-mapping>
    </service-endpoint-interface-mapping>
</java-wsdl-mapping>


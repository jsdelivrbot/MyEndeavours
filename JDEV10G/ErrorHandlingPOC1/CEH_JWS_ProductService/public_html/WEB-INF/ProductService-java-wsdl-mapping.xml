        <java-type>com.emerson.schemas.common.envelope.PropertyType[]</java-type>
        <anonymous-type-qname>{http://schemas.emerson.com/common/envelope}headerPropertiesType&gt;headerProperty[0,unbounded]</anonymous-type-qname>
        <qname-scope>complexType</qname-scope>
    </java-xml-type-mapping>
    <exception-mapping>
        <exception-type>com.emerson.schemas.common.envelope.FaultType</exception-type>
        <wsdl-message xmlns:exMsg="http://services.emerson.com/product">exMsg:FaultMessage</wsdl-message>
        <constructor-parameter-order>
             <element-name>faultSeverity</element-name>
             <element-name>faultCode</element-name>
             <element-name>faultMessage</element-name>
             <element-name>faultSummary</element-name>
        </constructor-parameter-order>
